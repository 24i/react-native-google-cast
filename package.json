{
  "name": "react-native-google-cast",
  "version": "4.0.0-pre",
  "description": "React Native wrapper for the Google Cast SDK for iOS and Android",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "docs": "node scripts/docs.js && cd docs-website && yarn start",
    "docs:build": "node scripts/docs.js && cd docs-website && yarn build"
  },
  "contributors": [
    {
      "name": "Petr Bela",
      "email": "github@petrbela.com"
    },
    {
      "name": "Charlie L",
      "email": "carlyeah@me.com"
    }
  ],
  "license": "MIT",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "chromecast",
    "google",
    "cast",
    "google-cast"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:react-native-google-cast/react-native-google-cast.git"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": ">=15.1 || >=16.0.0",
    "react-native": ">=0.40"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "detox": "^10.0.7",
    "jest": "24.1.0",
    "jest-websocket-mock": "^1.1.1",
    "jsdoc-to-markdown": "^4.0.1",
    "metro-react-native-babel-preset": "0.51.1",
    "mock-socket": "^8.0.5",
    "react": "16.6.3",
    "react-native": "0.58.4",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-navigation": "^2.11.0",
    "react-native-video": "^4.3.1",
    "react-navigation": "^3.3.0",
    "react-test-renderer": "16.6.3"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "example/ios/build/Build/Products/Debug-iphonesimulator/RNGCPlayground.app",
        "build": "xcodebuild -workspace example/ios/RNGCPlayground.xcworkspace -scheme RNGCPlayground -configuration Debug -sdk iphonesimulator -derivedDataPath example/ios/build",
        "type": "ios.simulator",
        "name": "iPhone Xs"
      }
    },
    "test-runner": "jest"
  }
}
